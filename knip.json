{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "next": {
    "entry": [
      "app/**/page.tsx",
      "app/**/layout.tsx",
      "app/**/route.ts",
      "app/**/loading.tsx",
      "app/**/error.tsx",
      "app/**/not-found.tsx",
      "app/sitemap.ts",
      "app/robots.ts",
      "middleware.ts"
    ]
  },
  "entry": ["worker.ts", "open-next.config.ts"],
  "project": ["**/*.{ts,tsx}"],
  "ignore": [
    ".next/**",
    ".open-next/**",
    ".wrangler/**",
    "dist/**",
    "out/**",
    "cloudflare-env.d.ts",
    "next-env.d.ts",
    "lib/supabase/database.types.ts",
    "scraper/**",
    "components/ui/**",
    "lib/animations.ts"
  ],
  "ignoreDependencies": ["supabase", "tailwindcss", "tw-animate-css", "postcss"],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false
}
