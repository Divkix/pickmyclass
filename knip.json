{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "middleware.ts",
    "worker.ts"
  ],
  "project": ["**/*.{ts,tsx,js,jsx,mjs}"],
  "ignore": [
    ".next/**",
    ".open-next/**",
    ".wrangler/**",
    "dist/**",
    "out/**",
    "cloudflare-env.d.ts",
    "next-env.d.ts",
    "lib/supabase/database.types.ts",
    ".next/types/**",
    "scraper/**",
    "*.config.{ts,js,mjs}"
  ],
  "ignoreDependencies": ["supabase", "tailwindcss", "tw-animate-css"],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false
}
